{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es7",
      "dom"
    ],
    "rootDirs": [
      "./src"
    ],
    "baseUrl": "./src",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@repositories/*": [
        "repositories/*"
      ],
      "@stores/*": [
        "stores/*"
      ],
      "@typings/*": [
        "typings/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
    }
  }
}
